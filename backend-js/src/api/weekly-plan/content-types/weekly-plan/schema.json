{
  "kind": "collectionType",
  "collectionName": "weekly_plans",
  "info": {
    "singularName": "weekly-plan",
    "pluralName": "weekly-plans",
    "displayName": "WeeklyPlan"
  },
  "options": {
    "draftAndPublish": true
  },
  "pluginOptions": {},
  "attributes": {
    "userId": {
      "type": "integer",
      "required": true
    },
    "weekStart": {
      "type": "date"
    },
    "weekEnd": {
      "type": "date"
    },
    "selections": {
      "type": "json"
    },
    "weeklyPlansStatus": {
      "type": "enumeration",
      "default": "draft ",
      "enum": [
        "draft ",
        "active",
        "completed",
        "archived"
      ]
    },
    "weekly_plan_meals": {
      "type": "relation",
      "relation": "oneToMany",
      "target": "api::weekly-plan-meal.weekly-plan-meal",
      "mappedBy": "weekly_plan"
    }
  }
}
